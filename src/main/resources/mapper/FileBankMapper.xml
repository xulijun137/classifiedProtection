<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jwell.classifiedProtection.mapper.FileBankMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jwell.classifiedProtection.entry.FileBank">
        <id column="id" property="id"/>
        <result column="create_time" property="createTime"/>
        <result column="file_name" property="fileName"/>
        <result column="file_path" property="filePath"/>
        <result column="table_name" property="tableName"/>
        <result column="foreign_id" property="foreignId"/>
        <result column="file_type" property="fileType"/>
        <result column="file_blob" property="fileBlob"/>
        <result column="desc" property="desc"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        create_time,
        id, file_name, file_path, table_name, foreign_id, file_type, file_blob, desc
    </sql>

</mapper>
